[alias]
  la = log --graph --decorate --pretty=oneline --abbrev-commit --all
  s = status
  sv = status --verbose
[filter "media"]
  required = true
  clean = git media clean %f
  smudge = git media smudge %f
[push]
  default = simple
[include]
  path = ~/etc/git/config.local
[user]
  name = Shawn O'Hare
  email = shawn@ohare.io
[credential]
  helper = cache --timeout 604800

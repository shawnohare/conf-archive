#!/usr/bin/env zsh

source "${ZPLUGIN_HOME}/bin/plug.zsh" cache
mkdir -p "${ZPLUGIN_HOME}/cache"
cache_file="${ZPLUGIN_HOME}/cache/plugins.zsh"
echo "Static file located at ${cache_file}"
rm "${cache_file}" > /dev/null
echo "# Automatically generated by plugin manager." >> "${cache_file}"
source "${ZDOTDIR}/plugins.zsh"
